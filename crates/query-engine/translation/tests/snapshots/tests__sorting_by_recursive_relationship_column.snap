WITH "%4_Company" AS (
  SELECT
    "%0_Company".*
  FROM
    "public"."Company" AS "%0_Company"
    LEFT OUTER JOIN LATERAL (
      SELECT
        "%2_ORDER_PART_Person"."Name" AS "Name"
      FROM
        (
          SELECT
            "%1_ORDER_PART_Person"."ParentId" AS "ParentId"
          FROM
            "public"."Person" AS "%1_ORDER_PART_Person"
          WHERE
            (
              "%0_Company"."CEOId" = "%1_ORDER_PART_Person"."PersonId"
            )
        ) AS "%1_ORDER_PART_Person"
        LEFT OUTER JOIN LATERAL (
          SELECT
            "%2_ORDER_PART_Person"."Name" AS "Name"
          FROM
            "public"."Person" AS "%2_ORDER_PART_Person"
          WHERE
            (
              "%1_ORDER_PART_Person"."ParentId" = "%2_ORDER_PART_Person"."PersonId"
            )
        ) AS "%2_ORDER_PART_Person" ON ('true')
    ) AS "%3_ORDER_FOR_Company" ON ('true')
  ORDER BY
    "%3_ORDER_FOR_Company"."Name" ASC
)
SELECT
  coalesce(json_agg(row_to_json("%9_universe")), '[]') AS "universe"
FROM
  (
    SELECT
      *
    FROM
      (
        SELECT
          coalesce(json_agg(row_to_json("%10_rows")), '[]') AS "rows"
        FROM
          (
            SELECT
              "%5_Company"."Name" AS "Name"
            FROM
              "%4_Company" AS "%5_Company"
              LEFT OUTER JOIN LATERAL (
                SELECT
                  "%7_ORDER_PART_Person"."Name" AS "Name"
                FROM
                  (
                    SELECT
                      "%6_ORDER_PART_Person"."ParentId" AS "ParentId"
                    FROM
                      "public"."Person" AS "%6_ORDER_PART_Person"
                    WHERE
                      (
                        "%5_Company"."CEOId" = "%6_ORDER_PART_Person"."PersonId"
                      )
                  ) AS "%6_ORDER_PART_Person"
                  LEFT OUTER JOIN LATERAL (
                    SELECT
                      "%7_ORDER_PART_Person"."Name" AS "Name"
                    FROM
                      "public"."Person" AS "%7_ORDER_PART_Person"
                    WHERE
                      (
                        "%6_ORDER_PART_Person"."ParentId" = "%7_ORDER_PART_Person"."PersonId"
                      )
                  ) AS "%7_ORDER_PART_Person" ON ('true')
              ) AS "%8_ORDER_FOR_Company" ON ('true')
            ORDER BY
              "%8_ORDER_FOR_Company"."Name" ASC
          ) AS "%10_rows"
      ) AS "%10_rows"
  ) AS "%9_universe";

