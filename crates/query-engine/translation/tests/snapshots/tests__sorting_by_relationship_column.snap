WITH "%3_Album" AS (
  SELECT
    "%0_Album".*
  FROM
    "public"."Album" AS "%0_Album"
    LEFT OUTER JOIN LATERAL (
      SELECT
        "%1_ORDER_PART_Artist"."Name" AS "Name"
      FROM
        (
          SELECT
            "%1_ORDER_PART_Artist"."Name" AS "Name"
          FROM
            "public"."Artist" AS "%1_ORDER_PART_Artist"
          WHERE
            (
              "%0_Album"."ArtistId" = "%1_ORDER_PART_Artist"."ArtistId"
            )
        ) AS "%1_ORDER_PART_Artist"
    ) AS "%2_ORDER_FOR_Album" ON ('true')
  ORDER BY
    "%2_ORDER_FOR_Album"."Name" ASC
  LIMIT
    5 OFFSET 3
)
SELECT
  coalesce(json_agg(row_to_json("%7_universe")), '[]') AS "universe"
FROM
  (
    SELECT
      *
    FROM
      (
        SELECT
          coalesce(json_agg(row_to_json("%8_rows")), '[]') AS "rows"
        FROM
          (
            SELECT
              "%4_Album"."Title" AS "Name"
            FROM
              "%3_Album" AS "%4_Album"
              LEFT OUTER JOIN LATERAL (
                SELECT
                  "%5_ORDER_PART_Artist"."Name" AS "Name"
                FROM
                  (
                    SELECT
                      "%5_ORDER_PART_Artist"."Name" AS "Name"
                    FROM
                      "public"."Artist" AS "%5_ORDER_PART_Artist"
                    WHERE
                      (
                        "%4_Album"."ArtistId" = "%5_ORDER_PART_Artist"."ArtistId"
                      )
                  ) AS "%5_ORDER_PART_Artist"
              ) AS "%6_ORDER_FOR_Album" ON ('true')
            ORDER BY
              "%6_ORDER_FOR_Album"."Name" ASC
          ) AS "%8_rows"
      ) AS "%8_rows"
  ) AS "%7_universe";

